{
    "name": "perform/perform-bundles",
    "description": "Perform Symfony bundles",
    "license": "proprietary",
    "authors": [
        {
            "name": "Glynn Forrest",
            "email": "me@glynnforrest.com"
        }
    ],
    "require": {
        "composer/semver": "^1.4",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "knplabs/knp-menu-bundle": "^2.2.1",
        "lyrixx/twitter-sdk": "^1.1",
        "nesbot/carbon": "^1.21",
        "oneup/flysystem-bundle": "^1.3",
        "pagerfanta/pagerfanta": "^1.0",
        "ralouphie/mimey": "^1.0",
        "sensio/framework-extra-bundle": "^5.0",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "^3.4",
        "symfony/form": "^3.4",
        "symfony/framework-bundle": "^3.4",
        "symfony/monolog-bundle": "^2.11",
        "symfony/security-bundle": "^3.4",
        "symfony/serializer": "^3.4",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/twig-bundle": "^3.4",
        "symfony/validator": "^3.4",
        "symfony/yaml": "^3.4"
    },
    "require-dev": {
        "doctrine/data-fixtures": "^1.2",
        "drewm/mailchimp-api": "^2.4",
        "fzaninotto/faker": "^1.5",
        "glynnforrest/temping": "^0.5.0",
        "imagine/imagine": "^0.6.3",
        "league/commonmark": "^0.17.0",
        "php-vfs/php-vfs": "^1.3",
        "phpdocumentor/reflection-docblock": "^3.1",
        "phpunit/phpunit": "^4.8",
        "sonata-project/exporter": "^1.7",
        "symfony/phpunit-bridge": "^4.0",
        "moneyphp/money": "^3.1"
    },
    "suggest": {
        "doctrine/data-fixtures": "Use database fixtures",
        "drewm/mailchimp-api": "Use the MailChimp mailing list connector",
        "fzaninotto/faker": "Use database fixtures",
        "imagine/imagine": "Use the image media plugin",
        "league/commonmark": "Use the markdown type in the base bundle",
        "sonata-project/exporter": "Export entities to a variety of formats",
        "moneyphp/money": "To use the money type"
    },
    "replace": {
        "perform/analytics-bundle": "self.version",
        "perform/base-bundle": "self.version",
        "perform/blog-bundle": "self.version",
        "perform/contact-bundle": "self.version",
        "perform/dev-bundle": "self.version",
        "perform/licensing": "self.version",
        "perform/mailing-list-bundle": "self.version",
        "perform/media-bundle": "self.version",
        "perform/notification-bundle": "self.version",
        "perform/page-editor-bundle": "self.version",
        "perform/rich-content-bundle": "self.version",
        "perform/spam-bundle": "self.version",
        "perform/user-bundle": "self.version"
    },
    "autoload": {
        "psr-4": {
            "Perform\\": "src"
        }
    }
}
