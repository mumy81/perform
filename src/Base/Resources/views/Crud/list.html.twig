{% extends 'AdminBaseBundle::base.html.twig' %}

{% block workspace %}
  <h3><i class="fa fa-angle-right"></i> Items</h3>
  <div class="row mt">
    <div class="col-lg-12">
      <div class="panel panel-default">
        <div class="panel-body">
          <table class="table">
            <thead>
              <th>
                ID
              </th>
            </thead>
            <tbody>
              {% for entity in entities %}
                <tr>
                  <td>{{entity.id}}</td>
                  <td><a href="view/{{entity.id}}">View</a></td>
                </tr>
              {% endfor %}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
{% endblock %}
